Cell["\<\
Now calculated the lead digit of the FileByteCount for each file (discarding \
folders and empty files).\
\>", "Text",ExpressionUUID->"098b522f-1fb6-49e4-97f5-79d2126f9428"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"digitCounts", "=", 
   RowBox[{"Counts", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"First", "[", 
       RowBox[{"IntegerDigits", "[", "#", "]"}], "]"}], "&"}], "/@", 
     RowBox[{"DeleteCases", "[", 
      RowBox[{
       RowBox[{"Quiet", "[", 
        RowBox[{"FileByteCount", "/@", "files"}], "]"}], ",", 
       RowBox[{"$Failed", "|", "0"}]}], "]"}]}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"digitFrequencies", "=", 
   RowBox[{"digitCounts", "/", 
    RowBox[{"Total", "[", "digitCounts", "]"}]}]}], ";"}]}], "Input",
 CellLabel->"In[31]:=",ExpressionUUID->"983ab6b9-4c65-42bd-9ff3-82218ae95dc8"],

Cell["\<\
Now use BenfordDistribution to generate the predicted proportions.\
\>", "Text",ExpressionUUID->"02c84eec-dd3e-4123-9579-fd7e75f3840b"],

Cell[BoxData[
 RowBox[{
  RowBox[{"predictions", "=", 
   RowBox[{"PDF", "[", 
    RowBox[{
     RowBox[{"BenfordDistribution", "[", "10", "]"}], ",", 
     RowBox[{"Range", "[", "9", "]"}]}], "]"}]}], ";"}]], "Input",
 CellLabel->"In[18]:=",ExpressionUUID->"7c15453a-aba2-4eb2-831b-5595f4c661e4"],

